#include <iostream>
#include <vector>

// Функція для перевірки, чи є число простим
bool isPrime(int num) {
    if (num <= 1) {
        return false;
    }
    if (num <= 3) {
        return true;
    }
    if (num % 2 == 0 || num % 3 == 0) {
        return false;
    }
    for (int i = 5; i * i <= num; i += 6) {
        if (num % i == 0 || num % (i + 2) == 0) {
            return false;
        }
    }
    return true;
}

int main() {
    // Ввід послідовності зі 100 чотиризначних чисел (в цьому прикладі ми генеруємо їх випадково)
    std::vector<int> sequence;
    for (int i = 0; i < 100; ++i) {
        int num = rand() % (10000 - 1000)+1000; // Генеруємо випадкове чотиризначне число
        sequence.push_back(num);
        std::cout << num << " ";
    }

    // Перевірка та виведення простих чисел
    std::cout << "Прості числа в послідовності:" << std::endl;
    for (int num : sequence) {
        if (isPrime(num)) {
            std::cout << num << " ";
        }
    }
    std::cout << std::endl;

    return 0;
}

